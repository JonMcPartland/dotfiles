#!/usr/bin/env bash

alias dotfiles="subl ~/.dotfiles"
alias dodotfiles=". ~/.bash_profile"
alias afk="/System/Library/CoreServices/Menu\ Extras/User.menu/Contents/Resources/CGSession -suspend"
alias colour="pygmentize -O style=monokai -f console256 -g" # (pipe it)
alias gif="convert -delay 20 -loop 0 $0 $1.gif"

alias dirsize="du -sh"
alias dirsizes="du -h -d=1"
alias bigfirst="sort -nk1"

alias fucking="sudo"
